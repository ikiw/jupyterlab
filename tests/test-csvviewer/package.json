{
  "name": "@jupyterlab/test-csvviewer",
  "version": "2.0.0-rc.1",
  "private": true,
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf build && rimraf coverage",
    "coverage": "python run-test.py --browsers=ChromeHeadless karma-cov.conf.js",
    "test": "jlpm run test:firefox",
    "test:chrome": "python run-test.py --browsers=Chrome karma.conf.js",
    "test:chrome-headless": "python run-test.py --browsers=ChromeHeadless karma.conf.js",
    "test:debug": "python run-test.py  --browsers=Chrome --singleRun=false --debug=true --browserNoActivityTimeout=10000000 karma.conf.js",
    "test:firefox": "python run-test.py --browsers=Firefox karma.conf.js",
    "test:ie": "python run-test.py  --browsers=IE karma.conf.js",
    "watch": "tsc -b --watch",
    "watch:src": "tsc -p src --watch"
  },
  "dependencies": {
    "@jupyterlab/csvviewer": "^2.0.0-rc.1",
    "@jupyterlab/docregistry": "^2.0.0-rc.1",
    "@jupyterlab/services": "^5.0.0-rc.1",
    "@jupyterlab/testutils": "^2.0.0-rc.1",
    "@lumino/coreutils": "^1.4.2",
    "@lumino/datagrid": "^0.4.0",
    "@lumino/widgets": "^1.10.2",
    "chai": "^4.2.0",
    "csv-spectrum": "~1.0.0",
    "simulate-event": "~1.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "puppeteer": "~2.0.0",
    "rimraf": "~3.0.0",
    "typescript": "~3.7.3"
  }
}
